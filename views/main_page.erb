<form action="/peep" method="POST">
  <input name="content" style="height: 50px; width: 150px;">
  <input type="submit" value="Submit" id="Submit">
</form>
<div class="peeps">
  <%@peeps.each do |peep| %>
    <div class="peep">
      <div class="peep-content">
        <%=peep.content%>
      </div>
      <div class="peep-date">
        <%=peep.date%>
      </div>
    </div>
  <%end%>
</div>